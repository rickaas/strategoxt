    s-enter(
    | "strategoxt/stratego-libraries/strc/tests/test1/io-test10.str"
    , "main"
    , "19"
    , "2"
    , "21"
    , "18"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/strc/tests/test1/io-test10.str"
      , "20"
      , "4"
      , "21"
      , "18"
      )
    ; (s-step(
       | "strategoxt/stratego-libraries/strc/tests/test1/io-test10.str"
