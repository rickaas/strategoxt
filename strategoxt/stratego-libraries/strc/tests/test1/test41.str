module test41

imports
  org.strategoxt.imp.debuggers.stratego.runtime


signature
  constructors
    Nil  : List(a)
    Cons : a * List(a) -> List(a)
    Zero : Nat
    Succ : Nat -> Nat
    Plus : Nat * Nat -> Nat


rules

  A :
    Plus(Zero, x) -> <r-exit(
                      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
                      , "A"
                      , "13"
                      , "2"
                      , "13"
                      , "23"
                      )> x
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test1/test41.str", "A", "13,2,13,23")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
             , "A"
             , "13"
             , "2"
             , "13"
             , "23"
             )
             ; fail

  B :
    Plus(Succ(x), y) -> <r-exit(
                         | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
                         , "B"
                         , "14"
                         , "2"
                         , "14"
                         , "41"
                         )> Succ(Plus(x, y))
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test1/test41.str", "B", "14,2,14,41")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
             , "B"
             , "14"
             , "2"
             , "14"
             , "41"
             )
             ; fail

  Ident :
    x -> <r-exit(
          | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
          , "Ident"
          , "16"
          , "2"
          , "16"
          , "15"
          )> x
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test1/test41.str", "Ident", "16,2,16,15")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
             , "Ident"
             , "16"
             , "2"
             , "16"
             , "15"
             )
             ; fail


strategies
  bottomup(s) =
    s-enter(
    | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
    , "bottomup"
    , "20"
    , "2"
    , "21"
    , "19"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "21"
      , "4"
      , "21"
      , "19"
      )
    ; rec x (
        s-step(
        | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
        , "21"
        , "10"
        , "21"
        , "18"
        )
        ; (s-step(
           | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
           , "21"
           , "10"
           , "21"
           , "15"
           )
           ; all(s-step(
                 | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
                 , "21"
                 , "14"
                 , "21"
                 , "14"
                 )
                 ; x))
        ; s-step(
          | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
          , "21"
          , "18"
          , "21"
          , "18"
          )
        ; s
      )
    < s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "bottomup"
      , "20"
      , "2"
      , "21"
      , "19"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "bottomup"
      , "20"
      , "2"
      , "21"
      , "19"
      )
      ; fail

  eval =
    s-enter(
    | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
    , "eval"
    , "23"
    , "2"
    , "24"
    , "28"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "24"
      , "4"
      , "24"
      , "28"
      )
    ; bottomup(
        s-step(
        | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
        , "24"
        , "13"
        , "24"
        , "27"
        )
        ; (s-step(
           | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
           , "24"
           , "13"
           , "24"
           , "13"
           )
           ; A
           <+ s-step(
              | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
              , "24"
              , "18"
              , "24"
              , "27"
              )
              ; (s-step(
                 | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
                 , "24"
                 , "18"
                 , "24"
                 , "18"
                 )
                 ; B
                 <+ s-step(
                    | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
                    , "24"
                    , "23"
                    , "24"
                    , "27"
                    )
                    ; Ident))
      )
    < s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "eval"
      , "23"
      , "2"
      , "24"
      , "28"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "eval"
      , "23"
      , "2"
      , "24"
      , "28"
      )
      ; fail

  main =
    s-enter(
    | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
    , "main"
    , "26"
    , "2"
    , "28"
    , "7"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "27"
      , "4"
      , "28"
      , "7"
      )
    ; (s-step(
       | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
       , "27"
       , "4"
       , "27"
       , "56"
       )
       ; !Plus(
            Succ(
              Plus(Succ(Zero), Succ(Zero))
            )
          , Succ(Zero)
          ))
    ; s-step(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "28"
      , "4"
      , "28"
      , "7"
      )
    ; eval
    < s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "main"
      , "26"
      , "2"
      , "28"
      , "7"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/strc/tests/test1/test41.str"
      , "main"
      , "26"
      , "2"
      , "28"
      , "7"
      )
      ; fail