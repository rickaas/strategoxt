          | "strategoxt/stratego-libraries/strc/tests/test1/test96.str"
          , "17"
          , "6"
          , "18"
          , "21"
          )
          ; (s-step(
             | "strategoxt/stratego-libraries/strc/tests/test1/test96.str"
