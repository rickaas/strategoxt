rules

  A :
    Plus(Zero, x) -> <r-exit(
                      | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
                      , "A"
                      , "26"
                      , "2"
                      , "26"
                      , "23"
                      )> x
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str", "A", "26,2,26,23")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
             , "A"
             , "26"
             , "2"
             , "26"
             , "23"
             )
             ; fail

  B :
    Plus(Succ(x), y) -> <r-exit(
                         | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
                         , "B"
                         , "28"
                         , "2"
                         , "29"
                         , "22"
                         )> Succ(Plus(x, z))
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str", "B", "28,2,29,22")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
             , "B"
             , "28"
             , "2"
             , "29"
             , "22"
             )
             ; fail
    where s-step(
          | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
          , "29"
          , "12"
          , "29"
          , "22"
          )
          ; (s-step(
             | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
             , "29"
             , "12"
             , "29"
             , "17"
             )
             ; <(s-step(
                 | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
                 , "29"
                 , "13"
                 , "29"
                 , "14"
                 )
                 ; id)> y) => z
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test2/conditional-innermost.str"
             , "B"
             , "28"
             , "2"
             , "29"
             , "22"
             )
             ; fail


