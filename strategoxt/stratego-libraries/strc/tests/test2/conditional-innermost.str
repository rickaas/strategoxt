module conditional-innermost

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  list-cons


signature
  sorts
    Nat

  constructors
    Zero : Nat
    Succ : Nat -> Nat
    Plus : Nat * Nat -> Nat


