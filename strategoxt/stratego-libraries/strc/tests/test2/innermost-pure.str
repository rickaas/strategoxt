module innermost-pure

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  libstratego-lib


signature
  sorts
    Nat

  constructors
    Zero : Nat
    Succ : Nat -> Nat
    Plus : Nat * Nat -> Nat


rules

  A :
    Plus(Zero, x) -> <r-exit(
                      | "strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str"
                      , "A"
                      , "11"
                      , "2"
                      , "11"
                      , "23"
                      )> x
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str", "A", "11,2,11,23")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str"
             , "A"
             , "11"
             , "2"
             , "11"
             , "23"
             )
             ; fail

  B :
    Plus(Succ(x), y) -> <r-exit(
                         | "strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str"
                         , "B"
                         , "13"
                         , "2"
                         , "13"
                         , "41"
                         )> Succ(Plus(x, y))
    where r-enter(|"strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str", "B", "13,2,13,41")
          <+ r-exit(
             | "strategoxt/stratego-libraries/strc/tests/test2/innermost-pure.str"
             , "B"
             , "13"
             , "2"
             , "13"
             , "41"
             )
             ; fail


