             | "strategoxt/stratego-libraries/java-backend/test/strc1/test54.str"
             , "A"
             , "21"
             , "2"
             , "21"
             , "23"
             )
             ; fail

  B :
    Plus(Succ(x), y) -> <r-exit(
                         | "strategoxt/stratego-libraries/java-backend/test/strc1/test54.str"
                         , "B"
                         , "22"
                         , "2"
                         , "22"
                         , "41"
                         )> Plus(x, Succ(y))
    where r-enter(|"strategoxt/stratego-libraries/java-backend/test/strc1/test54.str", "B", "22,2,22,41")
