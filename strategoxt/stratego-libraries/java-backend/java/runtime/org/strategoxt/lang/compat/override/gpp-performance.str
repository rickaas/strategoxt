module gpp-performance

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  libstratego-gpp


rules

  override gpp-topdown-fputs(|stream) =
             s-enter(
             | "strategoxt/stratego-libraries/java-backend/java/runtime/org/strategoxt/lang/compat/override/gpp-performance.str"
             , "gpp-topdown-fputs"
             , "8"
             , "11"
             , "9"
             , "40"
             )
             ; s-step(
               | "strategoxt/stratego-libraries/java-backend/java/runtime/org/strategoxt/lang/compat/override/gpp-performance.str"
               , "9"
               , "4"
               , "9"
               , "40"
               )
             ; prim("SSL_EXT_topdown_fputs", stream)
             < s-exit(
               | "strategoxt/stratego-libraries/java-backend/java/runtime/org/strategoxt/lang/compat/override/gpp-performance.str"
               , "gpp-topdown-fputs"
               , "8"
               , "11"
               , "9"
               , "40"
               )
             + s-exit(
               | "strategoxt/stratego-libraries/java-backend/java/runtime/org/strategoxt/lang/compat/override/gpp-performance.str"
               , "gpp-topdown-fputs"
               , "8"
               , "11"
               , "9"
               , "40"
               )
               ; fail