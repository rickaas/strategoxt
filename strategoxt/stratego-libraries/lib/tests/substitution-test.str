module substitution-test

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  libstratego-lib


strategies
  main-substitution-test =
    s-enter(
    | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
    , "main-substitution-test"
    , "5"
    , "2"
    , "8"
    , "4"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "6"
      , "4"
      , "8"
      , "4"
      )
    ; test-suite(
        s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "6"
        , "15"
        , "6"
        , "34"
        )
        ; !"substitution-test"
      , s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "7"
        , "1"
        , "7"
        , "5"
        )
        ; test1
      )
    < s-exit(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "main-substitution-test"
      , "5"
      , "2"
      , "8"
      , "4"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "main-substitution-test"
      , "5"
      , "2"
      , "8"
      , "4"
      )
      ; fail


signature
  constructors
    Typed  : Exp * Type -> Exp
    Var    : String -> Exp
    Forall : String * Type -> Type
    TVar   : String -> Type


strategies
  test1 =
    s-enter(
    | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
    , "test1"
    , "19"
    , "2"
    , "24"
    , "1"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "20"
      , "4"
      , "24"
      , "1"
      )
    ; apply-test(
        s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "20"
        , "15"
        , "20"
        , "22"
        )
        ; !"test1"
      , s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "21"
        , "2"
        , "21"
        , "21"
        )
        ; substitute(
            s-step(
            | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
            , "21"
            , "13"
            , "21"
            , "20"
            )
            ; TVar(
                s-step(
                | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
                , "21"
                , "18"
                , "21"
                , "19"
                )
                ; id
              )
          )
      , s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "22"
        , "2"
        , "22"
        , "67"
        )
        ; !( [TVar("b")]
           , [TVar("e")]
           , Typed(
               Var("a")
             , Forall("b", TVar("b"))
             )
           )
      , s-step(
        | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
        , "23"
        , "2"
        , "23"
        , "39"
        )
        ; !Typed(
             Var("a")
           , Forall("b", TVar("e"))
           )
      )
    < s-exit(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "test1"
      , "19"
      , "2"
      , "24"
      , "1"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/lib/tests/substitution-test.str"
      , "test1"
      , "19"
      , "2"
      , "24"
      , "1"
      )
      ; fail