    | "strategoxt/stratego-libraries/lib/spec/system/io/dir.str"
    , "remove-file"
    , "10"
    , "2"
    , "11"
    , "34"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/system/io/dir.str"
      , "11"
      , "4"
      , "11"
      , "34"
      )
