    s-enter(
    | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
    , "rename-file"
    , "22"
    , "2"
    , "24"
    , "41"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "23"
      , "4"
      , "24"
      , "41"
      )
    ; (s-step(
       | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
       , "23"
       , "4"
       , "23"
       , "22"
       )
       ; ?(oldname, newname))
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "24"
      , "6"
      , "24"
      , "41"
      )
    ; prim("SSL_rename", oldname, newname)
    < s-exit(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "rename-file"
      , "22"
      , "2"
      , "24"
      , "41"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "rename-file"
      , "22"
      , "2"
      , "24"
      , "41"
      )
      ; fail