module system/c99/file

imports
  org.strategoxt.imp.debuggers.stratego.runtime


strategies
  copy-file =
    s-enter(
    | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
    , "copy-file"
    , "15"
    , "2"
    , "17"
    , "39"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "16"
      , "4"
      , "17"
      , "39"
      )
    ; (s-step(
       | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
       , "16"
       , "4"
       , "16"
       , "22"
       )
       ; ?(oldname, newname))
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "17"
      , "6"
      , "17"
      , "39"
      )
    ; prim("SSL_copy", oldname, newname)
    < s-exit(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "copy-file"
      , "15"
      , "2"
      , "17"
      , "39"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/lib/spec/system/c99/file.str"
      , "copy-file"
      , "15"
      , "2"
      , "17"
      , "39"
      )
      ; fail

