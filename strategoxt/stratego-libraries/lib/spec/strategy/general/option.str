module strategy/general/option

imports
  org.strategoxt.imp.debuggers.stratego.runtime


signature
  sorts
    Option(a)

  constructors
    None : Option(a)
    Some : a -> Option(a)


strategies
  option(s) =
    s-enter(
    | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
    , "option"
    , "9"
    , "2"
    , "9"
    , "27"
    )
    ; s-step(
      | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
      , "9"
      , "14"
      , "9"
      , "27"
      )
    ; (s-step(
       | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
       , "9"
       , "14"
       , "9"
       , "17"
       )
       ; None
       + s-step(
         | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
         , "9"
         , "21"
         , "9"
         , "27"
         )
         ; Some(
             s-step(
             | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
             , "9"
             , "26"
             , "9"
             , "26"
             )
             ; s
           ))
    < s-exit(
      | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
      , "option"
      , "9"
      , "2"
      , "9"
      , "27"
      )
    + s-exit(
      | "strategoxt/stratego-libraries/lib/spec/strategy/general/option.str"
      , "option"
      , "9"
      , "2"
      , "9"
      , "27"
      )
      ; fail