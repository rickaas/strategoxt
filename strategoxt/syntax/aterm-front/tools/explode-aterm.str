module explode-aterm

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  libstratego-lib
  libstratego-aterm


strategies
  io-explode-aterm =
    s-enter(
    | "strategoxt/syntax/aterm-front/tools/explode-aterm.str"
    , "io-explode-aterm"
    , "8"
    , "2"
    , "9"
    , "25"
    )
    ; s-step(
      | "strategoxt/syntax/aterm-front/tools/explode-aterm.str"
      , "9"
      , "4"
      , "9"
      , "25"
      )
    ; io-wrap(
        s-step(
        | "strategoxt/syntax/aterm-front/tools/explode-aterm.str"
        , "9"
        , "12"
        , "9"
        , "24"
        )
        ; explode-aterm
      )
    < s-exit(
      | "strategoxt/syntax/aterm-front/tools/explode-aterm.str"
      , "io-explode-aterm"
      , "8"
      , "2"
      , "9"
      , "25"
      )
    + s-exit(
      | "strategoxt/syntax/aterm-front/tools/explode-aterm.str"
      , "io-explode-aterm"
      , "8"
      , "2"
      , "9"
      , "25"
      )
      ; fail