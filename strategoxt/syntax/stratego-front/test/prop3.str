module prop

imports
  org.strategoxt.imp.debuggers.stratego.runtime


rules

  DN :
    Not(Not(p)) -> <r-exit(
                    | "strategoxt/syntax/stratego-front/test/prop3.str"
                    , "DN"
                    , "4"
                    , "2"
                    , "4"
                    , "22"
                    )> p
    where r-enter(|"strategoxt/syntax/stratego-front/test/prop3.str", "DN", "4,2,4,22")
          <+ r-exit(
             | "strategoxt/syntax/stratego-front/test/prop3.str"
             , "DN"
             , "4"
             , "2"
             , "4"
             , "22"
             )
             ; fail

  DAOL :
    And(p, Or(q, r)) -> <r-exit(
                         | "strategoxt/syntax/stratego-front/test/prop3.str"
                         , "DAOL"
                         , "6"
                         , "2"
                         , "6"
                         , "49"
                         )> Or(
                              And(p, q)
                            , And(p, r)
                            )
    where r-enter(|"strategoxt/syntax/stratego-front/test/prop3.str", "DAOL", "6,2,6,49")
          <+ r-exit(
             | "strategoxt/syntax/stratego-front/test/prop3.str"
             , "DAOL"
             , "6"
             , "2"
             , "6"
             , "49"
             )
             ; fail